# LED node description
# TODO: .include xtea.nodl
# TODO: .include common.nodl

# general nodl information
nodl
{
name = led
}

# security
secu
{
cipher_alg = xtea
}

# control the led status
keyval
{
name = led_status
key = 0x0000
fmt = uint32
get = yes
set = yes
ack = yes
}

# periodically sent key
keyval
{
name = magic_value
key = 0x0001
fmt = uint32
}
